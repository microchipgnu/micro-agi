{
  "include": ["src/**/*"],
  "compilerOptions": {
    "lib": ["es2023"],
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "jsx": "react-jsx",
    "jsxImportSource": "ai-jsx",
    "module": "Node16",
    "resolveJsonModule": true,
    "declaration": true,
    "noEmit": false,
  }
}

