{
  "name": "micro-agi",
  "version": "0.0.0-alpha.14",
  "type": "module",
  "license": "MIT",
  "module": "./src/index.js",
  "scripts": {
    "typecheck": "tsc",
    "start": "bun run typecheck && bun run ./src/index.tsx",
    "start:debug": "bun run typecheck && AIJSX_LOG=debug bun run ./src/index.tsx",
    "build": "rm -rf dist && bun run typecheck && cp tsconfig.json dist/ && cp package.json dist/",
    "build:publish": "bun run build && npm publish -p ./dist"
  },
  "dependencies": {
    "@langchain/community": "^0.0.17",
    "ai-jsx": "^0.29",
    "langchain": "^0.1.3",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.4",
    "openai": "^4.26.0",
    "react": "^18.2.0",
    "type-fest": "^4.10.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.48",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  }
}
